import functools
def do_twice(func):
    @functools.wraps(func) # to preserve docs of func and whatnot
    def wrapper_do_twice(*args, **kwargs):
        func(*args, **kwargs)
        return func(*args, **kwargs)
    return wrapper_do_twice

